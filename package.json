  "devDependencies": {
    "@types/ioredis": "^4.26.4",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-window": "^1.8.3",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "7.28.0",
    "eslint-config-google": "^0.14.0",
